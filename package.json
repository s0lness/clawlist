{
  "name": "clawlist",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "npm run build:ui && tsc -p tsconfig.json",
    "build:ui": "node scripts/build-ui.js",
    "agent": "node dist/agent.js",
    "demo:inner": "npm run build && node scripts/demo.js",
    "demo": "bash scripts/demo-run.sh",
    "demo:docker": "bash scripts/demo-docker.sh",
    "demo:llm-buyer": "bash scripts/demo-llm-buyer.sh",
    "demo:llm-seller": "bash scripts/demo-llm-seller.sh",
    "demo:all": "bash scripts/reset-all.sh",
    "openclaw:bridge": "node dist/agent.js bridge --config config/agent_b.json --session matrix-marketplace",
    "openclaw:start": "bash scripts/openclaw-start.sh",
    "openclaw:check": "bash scripts/openclaw-check.sh",
    "test:protocol": "node scripts/test-protocol.js",
    "test:ui": "node scripts/test-ui.js",
    "test:e2e": "node scripts/test-e2e.js",
    "intake": "node dist/agent.js intake",
    "approve": "node dist/agent.js approve",
    "openclaw:intake": "bash scripts/openclaw-intake.sh",
    "telegram:relay": "node scripts/telegram-relay.js",
    "telegram:relay:setup": "node scripts/telegram-relay-setup.js",
    "telegram:relay:remove": "node scripts/telegram-relay-remove.js",
    "ui": "node scripts/ui-server.js",
    "start-all": "bash scripts/start-all.sh",
    "reset-all": "bash scripts/reset-all.sh"
  },
  "dependencies": {
    "matrix-js-sdk": "^40.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "typescript": "^5.5.4"
  }
}
